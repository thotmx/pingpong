<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Opponent</th>
      <th>Score</th>
      <th>Result</th>
    </tr>
  </thead>
  <tbody>
    <% @games. each do |game_raw| %>
      <% game = GameDecorator.new(game_raw, current_user) %>
      <tr>
        <td><%= l game.date %></td>
        <td><%= game.opponent %></td>
        <td><%= game.score %></td>
        <td><%= game.result %></td>
      </tr>
    <% end %>
  </tbody>
</table>

